<main class="content">
  <div class="container-fluid p-0">
    <h1 class="h3 mb-3">Liste des commandes</h1>
    <div class="row">
      <div class="col-12">
        <div class="card">
          <div class="table-responsive">
            <table class="table table-striped table-hover">
              <thead>
                <tr>
                  <th>Commande Id</th>
                  <th>Date de commande</th>
                  <th>Total</th>
                  <th>Client</th>
                  <th>Status</th>
                </tr>
              </thead>
              <tbody>
                <%= render('order_list') %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</main>
